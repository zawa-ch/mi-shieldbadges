def hex_to_rgb:
	if test("^[a-fA-F\\d]{6}$") then
		capture("(?<r>[a-fA-F\\d]{2})(?<g>[a-fA-F\\d]{2})(?<b>[a-fA-F\\d]{2})") | map_values(hex_tonumber)
	else
		error("Input is not a hex color code")
	end
;
def rgb_illuminance:
	0.2989*.r+0.5866*.g+0.1144*.b
